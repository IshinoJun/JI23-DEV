{
  "name": "JI23-DEV",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:path": "pathpida --watch",
    "build": "pathpida && next build",
    "start": "next start",
    "lint": "npm run lint:es && npm run lint:style",
    "lint:fix": "npm run lint:es:fix && npm run lint:style:fix",
    "lint:es": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:es:fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:style": "stylelint 'src/**/*.{css,scss}'",
    "lint:style:fix": "stylelint --fix 'src/**/*.{css,scss}'",
    "lint:conflict": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "postinstall": "typesync",
    "now-build": "cp canvas_lib64/*so.1 node_modules/canvas/build/Release/ && yarn build",
    "postbuild": "next-sitemap"
  },
  "dependencies": {
    "@hookform/resolvers": "^1.3.0",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@types/highlightjs": "^10.1.0",
    "@types/lodash": "^4.14.166",
    "@types/node": "^14.14.16",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.5",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/sass": "^1.16.0",
    "@types/yup": "^0.29.11",
    "axios": "0.21.1",
    "canvas": "^2.6.1",
    "classnames": "^2.2.6",
    "date-fns": "^2.16.1",
    "fs": "^0.0.1-security",
    "googleapis": "^66.0.0",
    "highlight.js": "^10.5.0",
    "html-to-text": "^6.0.0",
    "lodash": "^4.17.21",
    "next": "^11.1.1",
    "nprogress": "^0.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.13.1",
    "react-share": "^4.3.1",
    "react-stickynode": "^3.0.4",
    "ress": "^3.0.0",
    "sass": "^1.30.0",
    "typescript": "^4.1.3",
    "typesync": "^0.8.0",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/eslint": "^7.2.6",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/html-to-text": "^6.0.0",
    "@types/prettier": "^2.1.6",
    "@types/react-stickynode": "^3.0.1",
    "@types/stylelint": "^9.10.1",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "next-sitemap": "^1.3.24",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.15.0",
    "prettier": "^2.2.1",
    "stylelint": "^13.8.0",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{css,scss}": [
      "stylelint --fix",
      "git add"
    ]
  }
}
